<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.online_mobile_flexi_parking">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>-->

    <application
        android:name=".FireApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminMenu" />
        <activity android:name=".DeleteUser" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".ViewDatabaseUser" />
        <activity android:name=".RegisterEmailPassword" />
        <activity android:name=".ResetPage" />
        <activity android:name=".TestUser" />
        <activity android:name=".SendReset" />
        <activity android:name=".ChangeEmailPassword" />
        <activity android:name=".WatchTutorial" />
        <activity android:name=".AddModifyDeleteCarPlate" />
        <activity android:name=".UpdateCarPlate"></activity>
        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_place_api"></meta-data>
    </application>

</manifest>